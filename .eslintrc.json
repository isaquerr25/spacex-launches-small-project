

{
"env": {
    "browser": true,
    "es2021": true
},
"extends": [
    "airbnb-typescript-prettier",
    "prettier"
],
    "rules": {
        "react/jsx-filename-extension": [
        "error",
        {
            "extensions": [".js", ".jsx","tsx","ts"]
        }
        ],
        "global-require": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "warn",
        "multiline": true
    }

}
  